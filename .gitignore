$gi = @'
# Python
*.pyc
__pycache__/
*.pyo
*.pyd

# Virtualenvs
.venv/
venv/

# Envs and IDEs
.env
.vscode/
.idea/

# OS
.DS_Store
Thumbs.db

# Data / large files (keep sample only)
data/*.tsv
data/*.gz
mongo-data/

# Tests / coverage
.coverage
coverage/
.pytest_cache/

# Editor
*.swp
'@
Set-Content -Path .gitignore -Value $gi -Encoding UTF8